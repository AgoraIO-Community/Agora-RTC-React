{
  "name": "agora-rtc-react",
  "version": "2.0.0",
  "description": "A React wrapper for the Agora Web SDK",
  "files": [
    "dist"
  ],
  "main": "./dist/agora-rtc-react.umd.js",
  "module": "./dist/agora-rtc-react.es.js",
  "exports": {
    ".": {
      "import": "./dist/agora-rtc-react.es.js",
      "require": "./dist/agora-rtc-react.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite build --mode development",
    "build": "vite build",
    "docs": "typedoc src/index.tsx --categoryOrder 'Wrapper' --categoryOrder 'Components' --categoryOrder '*' --out docs"
  },
  "author": "EkaanshArora",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild": "^0.17.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typedoc": "^0.23.25",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-externalize-deps": "^0.5.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.24.9",
    "agora-rtc-sdk-ng": "^4.16.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.24.9",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
